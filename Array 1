#include<stdio.h>
#include<conio.h>
int main()
{
    system("cls");
    int arr[100], size, i=0;
    printf ("Enter the size of the array : ");
    scanf("%d", &size);

    printf ("Enter the elements of the array : \n");
    for (i=0;i<size;i++)
        scanf("%d", &arr[i]);
    printf ("Elements in Reverse order : \n");
    for (i=size-1;i>=0;i--)
        printf("%d \n", arr[i]);
    return 0;
}
