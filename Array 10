#include<stdio.h>
#include<conio.h>
int firstencounter(int n);
int arr[100],freq=1, size,ele, i=0, j=0;
int main()
{
    system("cls");

    printf ("Enter the size of the array : ");
    scanf("%d", &size);

    printf ("\n Enter the elements of the array : \n");
    for (i=0;i<size;i++)
        scanf("%d", &arr[i]);

    for (i=0;i<size;i++)
    {
        freq=1;
        for (j=i+1;j<size;j++)
        {
            if (arr[i]==arr[j])
            freq++;
        }
        if (firstencounter(i))
        {
            printf ("\n Frequency of Element %d is : ", arr[i]);
            printf("%d", freq);
        }
    }
    return 0;
}
int firstencounter(int n)
{
    int flag=0;
    for (i=0;i<n;i++)
    {
        if (arr[i]==arr[n])
        flag++;
    }
    if (flag==0)
    return 1;
    else
    return 0;
}
